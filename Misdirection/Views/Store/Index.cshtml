@model IEnumerable<Misdirection.Models.Genre>
@{
    ViewBag.Title = "Store";
}
<h3>Browse Genres</h3>

<ul class="small-block-grid-3 button-group" id="genre-buttons">
    @foreach (var genre in Model)
    {
        <li class="button">@Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name })</li>
    }
</ul>